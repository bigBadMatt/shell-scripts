#Matt's personal alias config
# :)

#make cp,mv and rm default to interactive mode
alias cp='cp -i'
alias mv='mv -i'
alias rm='rm -i'

#Git aliases
alias ga='git add'
alias gc='git commit -m'
alias gp='git push'

#clear the screen
alias c='clear'

#see a history of commands
alias hg='history | grep'

#emacs
alias e='emacs'